# Event Registration Configuration
# Set to "yes" to enable registration form for the latest event
NEXT_PUBLIC_ENABLE_EVENT_REGISTRATION=yes

# Convex Configuration
# Get this from https://dashboard.convex.dev
NEXT_PUBLIC_CONVEX_URL=your_convex_deployment_url_here

# ImageKit Configuration
# Get these from https://imagekit.io/dashboard
NEXT_PUBLIC_IMAGEKIT_PUBLIC_KEY=your_imagekit_public_key_here
IMAGEKIT_PRIVATE_KEY=your_imagekit_private_key_here
NEXT_PUBLIC_IMAGEKIT_URL_ENDPOINT=your_imagekit_url_endpoint_here